<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.mstore.ui.Store.StoreFragment"
        android:label="Store"
        tools:layout="@layout/fragment_store" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.mstore.ui.MyList.MyListFragment"
        android:label="My List"
        tools:layout="@layout/fragment_mylist" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.mstore.ui.Account.AccountFragment"
        android:label="Account"
        tools:layout="@layout/fragment_account" />
</navigation>